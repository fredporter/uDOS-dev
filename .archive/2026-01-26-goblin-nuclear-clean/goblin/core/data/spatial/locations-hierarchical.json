{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "title": "Earth Hierarchical Location System",
  "description": "120×50 cell grid with unlimited hierarchical depth via cell nesting",
  "version": "2.0.0",
  "last_updated": "2026-01-04",
  "system": "hierarchical_cells",

  "_COMMENT_HIERARCHICAL": "Each layer has 120×50 = 6,000 cells. Zoom into any cell creates 6,000 sub-cells. Format: L[LAYER]-[CELL1]-[CELL2]-[CELL3]...",
  "_COMMENT_CELL_FORMAT": "Cells are CCNN format (2 letters + 2 digits). Columns: AA-DT (120 columns), Rows: 00-49 (50 rows)",
  "_COMMENT_EXAMPLES": "L100 = World (6k cells), L100-AB34 = Australia (6k cells), L100-AB34-CD15 = Sydney (6k cells)",

  "grid_system": {
    "dimensions": {
      "columns": 120,
      "rows": 50,
      "total_cells_per_layer": 6000
    },
    "column_range": {
      "start": "AA",
      "end": "DT",
      "count": 120
    },
    "row_range": {
      "start": 0,
      "end": 49,
      "count": 50
    },
    "cell_format": "CCNN (e.g., AB15, CD20)",
    "cell_precision": "24×24 pixels at display scale",
    "coordinate_format": "[REALM]-[REGION]-L[LAYER]-[CELL1]-[CELL2]-[...]"
  },

  "base_layer": {
    "layer_id": 100,
    "name": "Earth World Layer",
    "realm": "EARTH",
    "cells": 6000,
    "precision_km": 333.33,
    "description": "Complete Earth surface at 333km/cell precision (40,000km circumference / 120 columns)"
  },

  "zoom_levels": {
    "depth_0": {
      "example": "EARTH-NA01-L100",
      "precision": "333 km per cell",
      "use_case": "Continental navigation, world overview"
    },
    "depth_1": {
      "example": "EARTH-NA01-L100-AB34",
      "precision": "2.77 km per cell (333/120)",
      "use_case": "Country/regional detail, state borders"
    },
    "depth_2": {
      "example": "EARTH-NA01-L100-AB34-CD15",
      "precision": "23 meters per cell (2.77km/120)",
      "use_case": "City metro areas, street level"
    },
    "depth_3": {
      "example": "EARTH-NA01-L100-AB34-CD15-AA20",
      "precision": "0.19 meters per cell (23m/120)",
      "use_case": "Building interiors, room level"
    },
    "depth_4": {
      "example": "EARTH-NA01-L100-AB34-CD15-AA20-BC10",
      "precision": "1.6 mm per cell (0.19m/120)",
      "use_case": "Object detail, microscopic if needed"
    }
  },

