{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "title": "Solar System Planets - Hierarchical Grid",
  "description": "Planetary bodies using 120×50 cell grid (6,000 cells per layer) with surface detail via cell nesting",
  "version": "2.0.0",
  "last_updated": "2026-01-04",
  "system": "hierarchical_cells",

  "grid_system": {
    "dimensions": {"columns": 120, "rows": 50},
    "cells_per_layer": 6000,
    "cell_format": "CCNN (AB15, CD20, etc.)",
    "coordinate_format": "SPACE-SOL-L[LAYER]-[CELL1]-[CELL2]...",
    "zoom_levels": {
      "depth_0": "Entire planet surface (6,000 cells)",
      "depth_1": "Continental/hemisphere detail (6,000 cells per base cell)",
      "depth_2": "Regional terrain (mountains, craters, landing sites)",
      "depth_3": "Local features (buildings, rovers, equipment)",
      "depth_4": "Object detail (tools, samples, instruments)"
    }
  },

  "layer_ranges": {
    "terrestrial": "600-609 (Mercury, Venus, Earth, Moon, Mars)",
    "asteroid_belt": "610-619 (Ceres, Vesta, etc.)",
    "jovian": "620-629 (Jupiter, Saturn, Uranus, Neptune)",
    "kuiper_belt": "630-639 (Pluto, Eris, Makemake, etc.)"
  },

