{
  "component": "groovebox",
  "version": "1.0.1",
  "description": "MML-based music production extension with 808/MC-303 groovebox workflow",
  "dependencies": {
    "core": ">=1.0.0",
    "api": ">=1.0.0",
    "tauri": ">=1.0.0",
    "wizard": ">=1.0.0"
  },
  "features": [
    "MML parser (complete syntax support)",
    "Sequencer (multi-track, loop, mute/solo)",
    "MIDI export (GM compatible)",
    "TR-808 drum machine (6 presets)",
    "Step grid conversion",
    "34 unit tests"
  ],
  "distribution": {
    "sounds": "wizard/web/catalog/groovebox/sounds/",
    "plugins": "wizard/web/catalog/groovebox/plugins/",
    "packs": "wizard/web/catalog/groovebox/packs/"
  }
}